type User {
  id: ID! @unique
  name: String!
  email: String!
}

type Recipe {
  id: ID! @unique
  title: String!
  description: String!
  preperationTime: Int
  cookingTime: Int
  image: String
  largeImage: String
  ingredients: [Json]
  directions: [String]
  #createdAt: DateTime!
  #updatedAt: DateTime!
  # user: User!
}